<root main_tree_to_execute = "MainTree">
    <BehaviorTree ID="MainTree">
            <Sequence>
                <!-- The goal value="position-x;position-y;orientaion-z;orientation-w" -->
                <SetBlackboard output_key="home" value="5.17;-0.2;0.0;1.0" />
                <SetBlackboard output_key="Goal_a" value="2.1;-0.73;0.0;1.0" />
                <SetBlackboard output_key="Goal_b" value="-0.7;1.5;0.0;1.0" />
                <Sequence>
                    <SubTree ID="MoveRobot" target="Goal_a" />
                    <SubTree ID="MoveRobot" target="Goal_b" />
                    <SubTree ID="MoveRobot" target="home" />
                    <AutodockClient action="start" />
                    <AutodockClient action="cancel" />
                    <SubTree ID="MoveRobot" target="Goal_a" />
                </Sequence>
            </Sequence>
    </BehaviorTree>

    <BehaviorTree ID="MoveRobot">
        <Sequence name="SetGoal">
            <Nav2Client goal="{target}" />
        </Sequence>
    </BehaviorTree>
</root>
